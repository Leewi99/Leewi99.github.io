<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" fillWidth="false" prefHeight="200.0" prefWidth="300.0" spacing="20.0" styleClass="vbox" stylesheets="@application/application.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.GameOverViewController">
   <children>
      <Label fx:id="TulosLabel" alignment="CENTER" styleClass="label" text="Pelin tulos">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="UusiPeliButton" mnemonicParsing="false" onAction="#aloitaUusiPeli" styleClass="button" text="Uusi peli">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
   </padding>
</VBox>
